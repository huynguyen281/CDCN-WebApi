pipeline {
    agent any
    stages {
        stage('Clone') {
            steps {
                git branch: 'develop', url: 'https://github.com/huypro020801/CDCN-WebApi.git'
            }
        }
        stage('Update DB') {
            steps {
                bat 'dotnet ef database update -p SpendingManagement/SpendingManagement.Data'
            }
        }
    }
}